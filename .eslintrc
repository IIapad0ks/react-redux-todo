{
  "parser"  : "babel-eslint",
  "extends" : "airbnb",
  "env"     : {
    "browser" : true,
    "node": true,
    "es6": true
  },
  "rules": {
    "jsx-a11y/img-uses-alt": 0,
    "jsx-a11y/redundant-alt": 0,
    "jsx-a11y/valid-aria-role": 0,
    "react/jsx-no-bind": 2,
    "consistent-return": 0,
    "no-unused-expressions": 0,
    "react/jsx-first-prop-new-line": 0,
    "indent": 2
  }
}
